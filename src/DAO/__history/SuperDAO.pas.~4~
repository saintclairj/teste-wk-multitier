unit SuperDAO;

interface
uses Firedac.Comp.Client, Data.DB;

  type

  TSuperDAO = class
    public
      function executarSQL(SQL: String): String;

  end;

implementation
uses Principal;

{ TSuperDAO }

function TSuperDAO.executarSQL(SQL: String): String;
var
  Query: TFDQuery;
begin
  Query:= TFDQuery.Create(frmPrincipal);
  Query.Connection := frmPrincipal.Conexao;
  Query.SQL.Text:= SQL;




end;

end.
